{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "max-line-length": [true, 120],
    "ordered-imports": [
      true,
      {
        "named-imports-order": "any"
      }
    ],

    "object-literal-sort-keys": false,
    "no-console": [true, "log"],
    "jsx-no-multiline-js": false,
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-template",
      "avoid-escape"
    ],
    "eofline": true
  },
  "rulesDirectory": []
}
